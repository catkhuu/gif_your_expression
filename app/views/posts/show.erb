<div class="container gif-show">
  <div class="section center-align">
    <div class="row">
      <div class="col s12 m4 18">
        <h3 class="center-align new-gif-title"><%= @post.title %></h3>
        <img class="center-align s12 m6 13" src="<%= @post.image_original_url %>" alt="<%= @post.title %>" style="width:<%= @post.image_width %>px;height:<%= @post.image_height %>px;">
      </div>
    </div>
  </div>
</div>
